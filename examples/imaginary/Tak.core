--tak :: Int -> Int -> Int -> Int
tak x y z = if not (y`lt'Int`x)
            then z
            else tak (tak (x-1) y z)
                     (tak (y-1) z x)
                     (tak (z-1) x y)

root x y z = tak x y z

tests = [
