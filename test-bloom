time ./test-all
time ./test-all --bloom --hist=0
time ./test-all --bloom --hist=1
time ./test-all --bloom --hist=2
time ./test-all --bloom --hist=3
time ./test-all --bloom --hist=4
time ./test-all --bloom --hist=5

