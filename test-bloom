time ./test-all --bloom --hist=2 --tag-collection=bags-strong
time ./test-all --bloom --hist=3 --tag-collection=bags-strong
time ./test-all --bloom --hist=4 --tag-collection=bags-strong
time ./test-all --bloom --hist=5 --tag-collection=bags-strong

